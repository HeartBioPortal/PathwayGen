{
  "source": {
    "include": ["src", "README.md"],
    "includePattern": ".js$",
    "excludePattern": "node_modules"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false
  },
  "opts": {
    "destination": "./docs/jsdoc",
    "recurse": true,
    "readme": "./README.md"
  }
}
